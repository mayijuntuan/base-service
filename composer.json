{
    "name": "mayijuntuan/base-service",
    "homepage": "https://github.com/mayijuntuan/base-service",
    "description": "Mayijuntuan Base Services in your PHP project",
    "keywords": ["mayiuntuan", "services", "sms","email","storage"],
    "license": "Apache-2.0",
    "type": "library",
    "authors": [
        {
            "name": "Mayijuntuan Base Services",
            "homepage": "http://www.mayijuntuan.com"
        }
    ],
    "support": {
        "source": "https://github.com/mayijuntuan/base-service",
        "issues": "https://github.com/mayijuntuan/base-service/issues"
    },
    "require": {
        "php": ">=5.5",
        "aws/aws-sdk-php": "^3.112",
        "qiniu/php-sdk": "^7.2",
        "alibabacloud/client": "^1.5",
        "guzzlehttp/guzzle": "^6.3",
        "aliyuncs/oss-sdk-php": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "classmap": [
            "src/Alipay/Request"
        ],
        "psr-4": {
            "Mayijuntuan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mayijuntuan\\Test\\": "tests/"
        }
    }
}
